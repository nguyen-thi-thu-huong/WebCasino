@model GameViewModel
@{
    ViewData["Title"] = "_GameBoardPartial";
}

<div id="winningAmount">@(Model.WinCoef * Model.BetAmount)</div>
<table class="table table-bordered">
    <thead>WIN: @Model.WinCoef</thead>
    @for (int i = 0; i < Model.Board.GetLength(0); i++)
    {
        <tr>
            @for (int j = 0; j < Model.Board.GetLength(1); j++)
            {
                <td>@Model.Board[i, j].SymbolName = @Model.Board[i, j].RolledNumber</td>
            }
        </tr>
    }

</table>

